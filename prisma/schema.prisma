generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id              String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  imageUrl        String
  height          Int
  width           Int
  croppedImageUrl String?
  createdAt       DateTime? @default(now()) @db.Timestamptz(6)
}
